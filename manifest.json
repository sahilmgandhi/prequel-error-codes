{
    "manifest_version": 2,
    "name": "PrequelErrorCodes",
    "version": "1.3",

    "description": "Change HTTP Error Codes to Prequel Memes",
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
       "128": "img/icon128.png"
      },
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["jquery.min.js", "content.js"],
        "matches": ["<all_urls>"]
    }]
}