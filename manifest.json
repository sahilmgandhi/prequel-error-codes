{
    "manifest_version": 2,
    "name": "PrequelErrorCodes",
    "version": "1.0",

    "description": "Change HTTP Error Codes to Prequel Memes",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["jquery.min.js", "content.js"],
        "matches": ["<all_urls>"]
    }]
}